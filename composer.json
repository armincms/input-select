{
    "name": "armincms/input-select",
    "description": "An another Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova",
        "fields",
        "select",
        "input",
        "armincms"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Armincms\\Fields\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Armincms\\Fields\\InputSelectServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
